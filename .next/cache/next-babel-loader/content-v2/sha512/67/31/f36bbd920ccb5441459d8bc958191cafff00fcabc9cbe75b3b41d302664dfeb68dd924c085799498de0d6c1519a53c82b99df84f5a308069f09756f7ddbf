ndler = async () => {\n    try {\n      console.log(account)\n      const getToken=await JuroTokenContract.methods.getToken().send({ from:account, value:'1000000000000000'})\n      const tokenAmount=await JuroTokenContract.methods.balanceOf(account).call()\n      setAmount(tokenAmount)\n\n      console.log(getToken)\n  \n\n    } catch (error) {\n      window.alert('Error occurred when buying')\n    }\n  }\n  \n  return (\n    <Web3ReactProvider getLibrary={getLibrary}>\n    <Layout account={account} login={connectHandler} logout={disconnectHandler}>\n      <Component {...pageProps} account={account} amount={amount} click={connectHandler} buyclick={buyHandler} contract={JuroTokenContract} logout={disconnectHandler} totalSupply={totalToken} connect={connect} disconnect={disconnect}/>\n    </Layout>\n    </Web3ReactProvider>\n  );\n}\n\nexport default MyApp;\n"]},"metadata":{},"sourceType":"module"}